{
  "compilerOptions": {
    "target": "ES2023",
    "lib": [
      "ES2023",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,

    "noEmit": true,
    "types": [
      "vite/client"
    ],
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src",
    "vite-env.d.ts"
  ]
}